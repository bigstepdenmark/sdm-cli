<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">

    <!-- COMMAND -->
    <type name="Magento\Framework\Console\CommandList">
        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="sdm_{{command_name}}_command" xsi:type="object">
                    SDM\{{module_name}}\Console\Command\{{module_name}}Command
                </item>
            </argument>
        </arguments>
    </type>

    <!-- LOGGER -->
    <type name="SDM\{{module_name}}\Logger\Handler\Info">
        <arguments>
            <argument name="filesystem" xsi:type="object">Magento\Framework\Filesystem\Driver\File</argument>
        </arguments>
    </type>
    <type name="SDM\{{module_name}}\Logger\Logger">
        <arguments>
            <argument name="name" xsi:type="string">{{logger_name}}</argument>
            <argument name="handlers" xsi:type="array">
                <!--emergency-->
                <!--alert-->
                <!--critical-->
                <!--error-->
                <!--warning-->
                <!--notice-->
                <item name="info" xsi:type="object">SDM\{{module_name}}\Logger\Handler\Info</item> <!--info-->
                <!--debug-->
            </argument>
        </arguments>
    </type>

    <!-- DI -->
    <preference for="SDM\{{module_name}}\Controller\Rest\RestInterface" type="SDM\{{module_name}}\Controller\Rest\Rest"/>

</config>
